          "id": "block_q8_intro",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  { "text": "Imaginons ensemble ton " },
                  { "bold": true, "text": "avenir ideal" },
                  { "text": "..." }
                ]
              },
              { "type": "p", "children": [{ "text": "" }] },
              {
                "type": "p",
                "children": [
                  { "text": "Avoir une " },
                  { "bold": true, "text": "vision claire" },
                  { "text": " de ou tu veux aller t'aide a orienter tes choix aujourd'hui." }
                ]
              },
              { "type": "p", "children": [{ "text": "" }] },
              {
                "type": "p",
                "children": [
                  { "text": "Laisse ton " },
