      "blocks": [
        {
          "id": "block_q6_intro",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  { "text": "Savoir poser des " },
                  { "bold": true, "text": "limites" },
                  { "text": " est essentiel pour des relations saines." }
                ]
              },
              { "type": "p", "children": [{ "text": "" }] },
              {
                "type": "p",
                "children": [
                  { "text": "C'est un acte de " },
                  { "bold": true, "text": "respect" },
                  { "text": " envers toi-meme et envers les autres." }
                ]
              }
            ]
          }
        },
        {
          "id": "block_q6_question",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [{ "bold": true, "text": "Sais-tu poser des limites claires dans tes relations ?" }]
              }
