              {
                "type": "p",
                "children": [
                  { "text": "Celui de ta " },
                  { "bold": true, "text": "connexion avec Allah" },
                  { "text": ", de tes " },
                  { "bold": true, "text": "ressources interieures" },
                  { "text": ", et de ta " },
                  { "bold": true, "text": "vision" },
                  { "text": " pour l'avenir." }
                ]
              }
            ]
          }
        },
        {
          "id": "block_verset_intro",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  {
                    "text": "\"Certes, dans l'evocation d'Allah les coeurs se tranquillisent.\" (13:28)",
                    "italic": true
                  }
                ]
              },
              { "type": "p", "children": [{ "text": "" }] },
              {
                "type": "p",
                "children": [
                  { "text": "Cette section va explorer comment tu nourris ton " },
                  { "bold": true, "text": "ame" },
                  { "text": " et quelles " },
                  { "bold": true, "text": "forces" },
                  { "text": " t'habitent." }
                ]
              }
            ]
          }
        },
        {
          "id": "block_intro_fin",
