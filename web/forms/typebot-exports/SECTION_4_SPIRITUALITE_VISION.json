          "items": [
            { "id": "soutien_mere", "content": "Ma mere" },
            { "id": "soutien_pere", "content": "Mon pere" },
            { "id": "soutien_fratrie", "content": "Une soeur/frere" },
            { "id": "soutien_amie", "content": "Une amie proche" },
            { "id": "soutien_famille", "content": "Un membre de la famille" },
            { "id": "soutien_therapeute", "content": "Un therapeute/coach" },
