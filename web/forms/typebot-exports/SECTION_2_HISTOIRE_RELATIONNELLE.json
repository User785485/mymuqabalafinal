      "id": "group_intro_histoire_relationnelle",
      "title": "Introduction Histoire Relationnelle",
      "graphCoordinates": { "x": 0, "y": 0 },
      "blocks": [
        {
          "id": "block_image_histoire",
          "type": "image",
          "content": {
            "url": "https://images.unsplash.com/photo-1518199266791-5375a83190b7?crop=entropy&cs=tinysrgb&fit=max&fm=jpg&w=1080"
          }
        },
        {
          "id": "block_titre_histoire",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [{ "bold": true, "text": "TON HISTOIRE D'AMOUR" }]
              }
            ]
          }
        },
        {
          "id": "block_intro_bienveillance",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  { "text": "Explorons ton " },
                  { "bold": true, "text": "parcours relationnel" },
                  { "text": " avec bienveillance..." }
                ]
              },
              { "type": "p", "children": [{ "text": "" }] },
              {
                "type": "p",
                "children": [
                  { "text": "Chaque relation que tu as vecue - " },
                  { "bold": true, "text": "quelle que soit sa duree ou son issue" },
                  { "text": " - t'a appris quelque chose sur toi-meme." }
                ]
              }
            ]
          }
        },
        {
          "id": "block_pourquoi_explorer",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [{ "bold": true, "text": "Pourquoi explorer ton histoire ?" }]
              },
              { "type": "p", "children": [{ "text": "" }] },
              {
                "type": "p",
                "children": [
                  { "text": "Parce que comprendre tes " },
                  { "bold": true, "text": "experiences passees" },
                  { "text": " te permet de ne plus les " },
                  { "bold": true, "text": "repeter inconsciemment" },
                  { "text": "." }
                ]
              },
              { "type": "p", "children": [{ "text": "" }] },
              {
                "type": "p",
                "children": [
                  { "text": "Il n'y a " },
                  { "bold": true, "text": "aucun jugement" },
                  { "text": " ici - seulement de la " },
                  { "bold": true, "text": "comprehension" },
                  { "text": "." }
                ]
              }
            ]
          }
        },
        {
          "id": "block_verset_histoire",
          "outgoingEdgeId": "edge_to_q1_nb_relations",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  {
                    "text": "\"Et rappelle ! Car le rappel profite aux croyants.\" (51:55)",
                    "italic": true
                  }
                ]
              },
              { "type": "p", "children": [{ "text": "" }] },
              {
                "type": "p",
                "children": [
                  { "text": "Se souvenir avec " },
                  { "bold": true, "text": "conscience" },
                  { "text": " est un acte de guerison." }
                ]
              }
            ]
          }
        }
      ]
    },
    {
      "id": "group_q1_nb_relations",
      "title": "Q1 - Nombre de relations",
      "graphCoordinates": { "x": 400, "y": 0 },
      "blocks": [
        {
          "id": "block_q1_question",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  { "bold": true, "text": "Combien de relations amoureuses serieuses as-tu eues jusqu'a present ?" }
                ]
              },
              { "type": "p", "children": [{ "text": "" }] },
              {
                "type": "p",
                "children": [
                  {
                    "text": "Par \"serieuse\", j'entends une relation ou il y avait un engagement emotionnel reel, meme si elle n'a pas dure longtemps.",
                    "italic": true
                  }
                ]
              }
            ]
          }
        },
        {
          "id": "block_q1_choix",
          "outgoingEdgeId": "edge_to_q2_duree",
          "type": "choice input",
          "items": [
            { "id": "nb_aucune", "content": "Aucune" },
            { "id": "nb_1_2", "content": "1 a 2" },
            { "id": "nb_3_5", "content": "3 a 5" },
            { "id": "nb_plus_5", "content": "Plus de 5" },
            { "id": "nb_prefere_pas", "content": "Je prefere ne pas repondre" }
          ],
          "options": { "variableId": "var_v3_nb_relations", "isMultipleChoice": false }
        }
      ]
    },
    {
      "id": "group_q2_duree_relations",
      "title": "Q2 - Duree des relations",
      "graphCoordinates": { "x": 800, "y": 0 },
      "blocks": [
        {
          "id": "block_q2_transition",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  { "text": "Merci pour cette reponse." }
                ]
              },
              { "type": "p", "children": [{ "text": "" }] },
              {
                "type": "p",
                "children": [
                  { "text": "La " },
                  { "bold": true, "text": "duree" },
                  { "text": " de tes relations peut reveler des patterns interessants..." }
                ]
              }
            ]
          }
        },
        {
          "id": "block_q2_question",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  { "bold": true, "text": "En moyenne, combien de temps duraient tes relations ?" }
                ]
              }
            ]
          }
        },
        {
          "id": "block_q2_choix",
          "outgoingEdgeId": "edge_to_q3_rupture",
          "type": "choice input",
          "items": [
            { "id": "duree_semaines", "content": "Quelques semaines" },
            { "id": "duree_1_6_mois", "content": "Quelques mois (1-6)" },
            { "id": "duree_6_12_mois", "content": "6 mois a 1 an" },
            { "id": "duree_1_3_ans", "content": "1 a 3 ans" },
            { "id": "duree_plus_3_ans", "content": "Plus de 3 ans" }
          ],
          "options": { "variableId": "var_v3_duree_relations", "isMultipleChoice": false }
        }
      ]
    },
    {
      "id": "group_q3_raisons_rupture",
      "title": "Q3 - Raisons de rupture",
      "graphCoordinates": { "x": 1200, "y": 0 },
      "blocks": [
        {
          "id": "block_q3_intro",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  { "text": "Je comprends que cette question peut etre " },
                  { "bold": true, "text": "delicate" },
                  { "text": "..." }
                ]
              },
              { "type": "p", "children": [{ "text": "" }] },
              {
                "type": "p",
                "children": [
                  { "text": "Mais comprendre " },
                  { "bold": true, "text": "comment" },
                  { "text": " et " },
                  { "bold": true, "text": "pourquoi" },
                  { "text": " tes relations se sont terminees est essentiel pour ne pas " },
                  { "bold": true, "text": "repeter les memes schemas" },
                  { "text": "." }
                ]
              }
            ]
          }
        },
        {
          "id": "block_q3_question",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  { "bold": true, "text": "Comment se terminaient generalement tes relations ?" }
                ]
              },
              { "type": "p", "children": [{ "text": "" }] },
              {
                "type": "p",
                "children": [
                  { "text": "Tu peux selectionner plusieurs reponses si differentes situations se sont produites.", "italic": true }
                ]
              }
            ]
          }
        },
        {
          "id": "block_q3_choix",
          "outgoingEdgeId": "edge_to_q4_marquante",
          "type": "choice input",
          "items": [
            { "id": "rupture_moi", "content": "C'est moi qui partais" },
            { "id": "rupture_lui", "content": "C'est lui qui partait" },
            { "id": "rupture_mutuelle", "content": "Decision mutuelle" },
            { "id": "rupture_ghosting", "content": "Ghosting/disparition" },
            { "id": "rupture_infidelite", "content": "Infidelite" },
            { "id": "rupture_distance", "content": "Distance/incompatibilite" },
            { "id": "rupture_communication", "content": "Problemes de communication" },
            { "id": "rupture_autre", "content": "Autre" }
          ],
          "options": { "variableId": "var_v3_raisons_rupture", "isMultipleChoice": true }
        }
      ]
    },
    {
      "id": "group_q4_relation_marquante",
      "title": "Q4 - Relation marquante",
      "graphCoordinates": { "x": 1600, "y": 0 },
      "blocks": [
        {
          "id": "block_q4_transition",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  { "text": "Merci pour cette transparence." }
                ]
              },
              { "type": "p", "children": [{ "text": "" }] },
              {
                "type": "p",
                "children": [
                  { "text": "Maintenant, j'aimerais que tu prennes un instant pour te " },
                  { "bold": true, "text": "rememorer" },
                  { "text": " la relation qui t'a le plus " },
                  { "bold": true, "text": "marquee" },
                  { "text": "..." }
                ]
              },
              { "type": "p", "children": [{ "text": "" }] },
              {
                "type": "p",
                "children": [
                  { "text": "Que ce soit de maniere " },
                  { "bold": true, "text": "positive" },
                  { "text": " ou " },
                  { "bold": true, "text": "negative" },
                  { "text": ", cette relation porte en elle des lecons precieuses." }
                ]
              }
            ]
          }
        },
        {
          "id": "block_q4_question",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  { "bold": true, "text": "Decris brievement la relation qui t'a le plus marquee (positive ou negative). Qu'as-tu appris ?" }
                ]
              }
            ]
          }
        },
        {
          "id": "block_q4_input",
          "outgoingEdgeId": "edge_to_q5_premier_amour",
          "type": "text input",
          "options": {
            "labels": { "placeholder": "Prends le temps de te rememorer cette relation...", "button": "Continuer" },
            "variableId": "var_v3_relation_marquante",
            "isLong": true
          }
        }
      ]
    },
    {
      "id": "group_q5_premier_amour",
      "title": "Q5 - Premier amour",
      "graphCoordinates": { "x": 2000, "y": 0 },
      "blocks": [
        {
          "id": "block_q5_transition",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  { "text": "Merci de me confier ces souvenirs." }
                ]
              },
              { "type": "p", "children": [{ "text": "" }] },
              {
                "type": "p",
                "children": [
                  { "text": "Notre " },
                  { "bold": true, "text": "premiere experience amoureuse significative" },
                  { "text": " laisse souvent une empreinte profonde..." }
                ]
              },
              { "type": "p", "children": [{ "text": "" }] },
              {
                "type": "p",
                "children": [
                  { "text": "Elle " },
                  { "bold": true, "text": "colore" },
                  { "text": " notre facon de voir l'amour par la suite." }
                ]
              }
            ]
          }
        },
        {
          "id": "block_q5_question",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  { "bold": true, "text": "Comment decrirais-tu ta premiere experience amoureuse significative ?" }
                ]
              }
            ]
          }
        },
        {
          "id": "block_q5_choix",
          "outgoingEdgeId": "edge_to_conclusion_s2",
          "type": "choice input",
          "items": [
            { "id": "premier_magique", "content": "Magique et fondatrice" },
            { "id": "premier_douloureuse", "content": "Douloureuse mais formatrice" },
            { "id": "premier_confuse", "content": "Confuse et maladroite" },
            { "id": "premier_decevante", "content": "Decevante" },
            { "id": "premier_pas_encore", "content": "Je n'en ai pas encore eu" },
            { "id": "premier_autre", "content": "Autre" }
          ],
          "options": { "variableId": "var_v3_premier_amour", "isMultipleChoice": false }
        }
      ]
    },
    {
      "id": "group_conclusion_s2",
      "title": "Conclusion Section 2",
      "graphCoordinates": { "x": 2400, "y": 0 },
      "blocks": [
        {
          "id": "block_conclusion_reconnaissance",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  { "text": "Merci pour cette " },
                  { "bold": true, "text": "plongee courageuse" },
                  { "text": " dans ton histoire relationnelle." }
                ]
              },
              { "type": "p", "children": [{ "text": "" }] },
              {
                "type": "p",
                "children": [
                  { "text": "Chaque experience - " },
                  { "bold": true, "text": "meme les plus difficiles" },
                  { "text": " - t'a appris quelque chose sur toi-meme et sur ce que tu recherches vraiment." }
                ]
              }
            ]
          }
        },
        {
          "id": "block_conclusion_sagesse",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  {
                    "text": "\"Peut-etre detestez-vous quelque chose alors qu'elle est un bien pour vous.\" (2:216)",
                    "italic": true
                  }
                ]
              },
              { "type": "p", "children": [{ "text": "" }] },
              {
                "type": "p",
                "children": [
                  { "text": "Les lecons de ton passe sont les " },
                  { "bold": true, "text": "pierres de ta sagesse" },
                  { "text": " future." }
                ]
              }
            ]
          }
        },
        {
          "id": "block_conclusion_fin",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  { "bold": true, "text": "Section Histoire Relationnelle terminee." }
                ]
              },
              { "type": "p", "children": [{ "text": "" }] },
              {
                "type": "p",
                "children": [
                  { "text": "Ces informations nous aideront a " },
                  { "bold": true, "text": "identifier tes patterns" },
                  { "text": " et a construire une " },
                  { "bold": true, "text": "vision plus claire" },
                  { "text": " de ton chemin vers l'amour." }
                ]
              }
            ]
          }
        }
      ]
    }
