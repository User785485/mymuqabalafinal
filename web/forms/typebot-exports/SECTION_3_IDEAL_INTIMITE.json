          "items": [
            {
              "id": "ci-item-1",
              "content": "Contact minimal (discussions)"
            },
            {
              "id": "ci-item-2",
              "content": "Gestes tendres (mains, calins)"
            },
            {
              "id": "ci-item-3",
              "content": "Proximite physique progressive"
            },
            {
              "id": "ci-item-4",
              "content": "Ca depend de la connexion"
            },
            {
              "id": "ci-item-5",
              "content": "Je prefere ne pas repondre"
            }
          ]
        }
