          "items": [
            {
              "id": "ri-item-1",
              "content": "C'est essentiel et naturel pour moi"
            },
            {
              "id": "ri-item-2",
              "content": "Important mais j'ai besoin de temps"
            },
            {
              "id": "ri-item-3",
              "content": "Sujet de questionnement"
            },
            {
              "id": "ri-item-4",
              "content": "Source d'anxiete"
            },
            {
              "id": "ri-item-5",
              "content": "J'attends le mariage"
            },
            {
              "id": "ri-item-6",
              "content": "Je prefere ne pas repondre"
            }
          ]
        }
