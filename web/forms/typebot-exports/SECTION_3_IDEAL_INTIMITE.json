          "items": [
            {
              "id": "ic-item-1",
              "content": "En paix et reconnaissante"
            },
            {
              "id": "ic-item-2",
              "content": "Generalement bien avec des moments de doute"
            },
            {
              "id": "ic-item-3",
              "content": "En travail d'acceptation"
            },
            {
              "id": "ic-item-4",
              "content": "Souvent en conflit"
            },
            {
              "id": "ic-item-5",
              "content": "C'est un sujet difficile"
            },
            {
              "id": "ic-item-6",
              "content": "Je prefere ne pas repondre"
            }
          ]
        }
      ]
