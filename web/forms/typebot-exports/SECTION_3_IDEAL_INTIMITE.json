  "groups": [
    {
      "id": "group-intro-ideal",
      "title": "Introduction Ideal Partenaire",
      "graphCoordinates": {
        "x": 0,
        "y": 0
      },
      "blocks": [
        {
          "id": "block-intro-ideal-1",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  {
                    "text": "Ton Ideal Amoureux",
                    "bold": true
                  }
                ]
              }
            ]
          }
        },
        {
          "id": "block-intro-ideal-2",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  {
                    "text": "Explorons ce que tu recherches vraiment chez un partenaire de vie..."
                  }
                ]
              }
            ]
          }
        },
        {
          "id": "block-intro-ideal-3",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  {
                    "text": "Prends le temps de reflechir a ce qui compte vraiment pour toi. Il n'y a pas de bonne ou mauvaise reponse, juste TA verite."
                  }
                ]
              }
            ]
          }
        }
      ]
    },
    {
      "id": "group-q1-ideal-partenaire",
      "title": "Q1 - Ideal Partenaire",
      "graphCoordinates": {
        "x": 0,
        "y": 200
      },
      "blocks": [
        {
          "id": "block-q1-question",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  {
                    "text": "Decris les 3 qualites essentielles que tu recherches chez ton partenaire ideal."
                  }
                ]
              }
            ]
          }
        },
        {
          "id": "block-q1-input",
          "type": "text input",
          "options": {
            "labels": {
              "placeholder": "Ex: La douceur, l'ambition, la foi...",
              "button": "Continuer"
            },
            "variableId": "var_v3_ideal_partenaire_id",
            "isLong": true
          }
        }
      ]
    },
    {
      "id": "group-q2-red-flags",
      "title": "Q2 - Red Flags",
      "graphCoordinates": {
        "x": 0,
        "y": 400
      },
      "blocks": [
        {
          "id": "block-q2-question",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  {
                    "text": "Quels signaux d'alarme (red flags) as-tu appris a reconnaitre dans une relation?"
                  }
                ]
              }
            ]
          }
        },
        {
          "id": "block-q2-subtext",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  {
                    "text": "Tu peux selectionner plusieurs reponses.",
                    "italic": true
                  }
                ]
              }
            ]
          }
        },
        {
          "id": "block-q2-choice",
          "type": "choice input",
          "options": {
            "isMultipleChoice": true,
            "buttonLabel": "Valider mes choix",
            "variableId": "var_v3_red_flags_id",
            "isSearchable": false
          },
          "items": [
            {
              "id": "rf-item-1",
              "content": "Manque de respect"
            },
            {
              "id": "rf-item-2",
              "content": "Jalousie excessive"
            },
            {
              "id": "rf-item-3",
              "content": "Controle/manipulation"
            },
            {
              "id": "rf-item-4",
              "content": "Mensonges repetes"
            },
            {
              "id": "rf-item-5",
              "content": "Manque d'engagement"
            },
            {
              "id": "rf-item-6",
              "content": "Violence verbale"
            },
            {
              "id": "rf-item-7",
              "content": "Indisponibilite emotionnelle"
            },
            {
              "id": "rf-item-8",
              "content": "Irresponsabilite financiere"
            },
            {
              "id": "rf-item-9",
              "content": "Incompatibilite de valeurs"
            },
            {
              "id": "rf-item-10",
              "content": "Autre"
            }
          ]
        }
      ]
    },
    {
      "id": "group-q3-non-negociables",
      "title": "Q3 - Non Negociables",
      "graphCoordinates": {
        "x": 0,
        "y": 600
      },
      "blocks": [
        {
          "id": "block-q3-question",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  {
                    "text": "Quels sont tes criteres absolument non negociables dans une relation?"
                  }
                ]
              }
            ]
          }
        },
        {
          "id": "block-q3-subtext",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  {
                    "text": "Ces elements sur lesquels tu ne feras jamais de compromis, car ils touchent a tes valeurs profondes.",
                    "italic": true
                  }
                ]
              }
            ]
          }
        },
        {
          "id": "block-q3-input",
          "type": "text input",
          "options": {
            "labels": {
              "placeholder": "Ex: La pratique religieuse, le respect, la fidelite...",
              "button": "Continuer"
            },
            "variableId": "var_v3_non_negociables_id",
            "isLong": true
          }
        }
      ]
    },
    {
      "id": "group-transition-intimite",
      "title": "Transition Corps/Intimite",
      "graphCoordinates": {
        "x": 0,
        "y": 800
      },
      "blocks": [
        {
          "id": "block-transition-1",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  {
                    "text": "Ton Corps et Toi",
                    "bold": true
                  }
                ]
              }
