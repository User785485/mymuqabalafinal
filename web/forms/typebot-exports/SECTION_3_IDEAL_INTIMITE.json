          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  {
                    "text": "Quel niveau d'intimite te met a l'aise dans les premiers temps d'une relation?"
                  }
                ]
              }
            ]
          }
        },
        {
          "id": "block-q6-choice",
          "type": "choice input",
          "options": {
            "isMultipleChoice": false,
            "variableId": "var_v3_confort_intimite_id"
