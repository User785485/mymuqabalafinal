        {
          "id": "block-transition-3",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  {
                    "text": "Ces questions sont delicates et personnelles. Reponds uniquement si tu te sens a l'aise. L'option \"Je prefere ne pas repondre\" est toujours disponible et totalement respectee."
                  }
                ]
              }
            ]
          }
        },
        {
          "id": "block-transition-4",
          "type": "text",
          "content": {
            "richText": [
              {
                "type": "p",
                "children": [
                  {
                    "text": "Tes reponses restent strictement confidentielles.",
                    "italic": true
                  }
                ]
              }
            ]
          }
        }
      ]
