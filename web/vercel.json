{
  "buildCommand": null,
  "outputDirectory": ".",
  "framework": null,
  "cleanUrls": true,
  "redirects": [
    {
      "source": "/",
      "has": [{ "type": "host", "value": "admin.my-muqabala.fr" }],
      "destination": "/admin",
      "permanent": false
    }
  ],
  "rewrites": [
    { "source": "/login", "destination": "/dashboard-login" },
    { "source": "/admin", "destination": "/admin/index.html" },
    { "source": "/app/:path*", "destination": "/app/index.html" }
  ],
  "headers": [
    {
      "source": "/app/assets/(.*)",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    },
    {
      "source": "/app/flutter_service_worker.js",
      "headers": [{ "key": "Cache-Control", "value": "no-cache, max-age=0" }]
    },
    {
      "source": "/app/flutter_bootstrap.js",
      "headers": [{ "key": "Cache-Control", "value": "no-cache, max-age=0" }]
    },
    {
      "source": "/app/main.dart.js",
      "headers": [{ "key": "Cache-Control", "value": "no-cache, max-age=0" }]
    }
  ]
}
